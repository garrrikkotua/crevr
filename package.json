{
  "name": "crev",
  "version": "0.0.1",
  "description": "A web-based UI for reviewing and reverting Claude Code changes",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "crev": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "revert",
    "undo",
    "diff",
    "cli",
    "ui",
    "web"
  ],
  "author": "Igor <garrrikkotua@github.com>",
  "license": "MIT",
  "homepage": "https://github.com/garrrikkotua/crev#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/garrrikkotua/crev.git"
  },
  "bugs": {
    "url": "https://github.com/garrrikkotua/crev/issues"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "open": "^9.1.0",
    "ws": "^8.16.0",
    "chalk": "^4.1.2",
    "diff": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/diff": "^5.0.9",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  },
  "files": [
    "dist",
    "public"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}